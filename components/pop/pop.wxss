

.masking{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0,0,0,.5);
  overflow: hidden;
  z-index: 100;
  color: #fff;
}
.maskingCon{
  width: 568rpx;
  height: 708rpx;
  background-color:#fff;
  z-index: 200; 
  position: absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
}
.evaluatePop{
  width: 100%;
  height: 264rpx;
  background-color:#EDF6EC;
}
.evaluatePop image{
    width: 185rpx;
    height: 141rpx;
    position: absolute;
    top:20%;
    left:50%;
    transform: translate(-50%,-50%);
}
.close{
  color:#B8DDB7;
  font-size: 27rpx;
  position: absolute;
  top:30rpx;
  right:30rpx;
}
.textCon{
  padding:0 50rpx;
}
.text1{
  padding-top:50rpx;
  color:#83C582;
  font-size: 28rpx;
}
.text2{
   padding-top:30rpx;
  color:#203736;
  font-size: 36rpx;
}
.text3{
   padding-top:30rpx;
  color:#B4B5B5;
  font-size: 28rpx;
}
.textCon button{
  width: 83%;
  font-size: 40rpx;
  position: absolute;
  bottom:50rpx;
}
.hid{
  display: none;
}